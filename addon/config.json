{
  "name": "TileBoard",
  "version": "1.3.8",
  "slug": "tileboard",
  "description": "Simple yet highly-customizable dashboard (MOD)",
  "url": "https://github.com/SlavaVolkov116/TileBoard-addon",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "stage": "experimental",
  "startup": "application",
  "init": false,
  "image": "tileboard/tileboard-addon-{arch}",
  "ingress": true,
  "homeassistant_api": true,
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "map": [
    "config",
    "share",
    "ssl"
  ],
  "boot": "auto",
  "ports": {
    "80/tcp": null
  },
  "ports_description": {
    "80/tcp": "Dashboard interface (not required for Ingress)"
  },
  "options": {
    "configFolder": "tileboard",
    "ssl": false,
    "useExternalUrl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "configFolder": "str",
    "ssl": "bool",
    "useExternalUrl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}
